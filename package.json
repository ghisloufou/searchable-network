{
  "name": "better-network",
  "version": "1.0.0",
  "description": "A chrome extension to better search in the network payloads and responses",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghisloufou/better-better-network-panel.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "buildw": "vite build -w",
    "typew": "tsc --noEmit -w --pretty",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "yarn build:hmr && (yarn wss & nodemon)",
    "devp": "pnpm build:hmr && (pnpm wss & nodemon)",
    "test": "jest",
    "start": "concurrently -n dev,typew,buildw \"yarn dev\" \"yarn typew\" \"yarn buildw\"",
    "startp": "concurrently -n dev,typew,buildw \"pnpm devp\" \"pnpm typew\" \"pnpm buildw\""
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@types/jsonpath": "^0.2.0",
    "bootstrap": "^5.2.2",
    "jsoneditor": "^9.9.2",
    "jsonpath": "^1.1.1",
    "react": "18.2.0",
    "react-diff-viewer-continued": "^3.2.5",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-icons": "^4.7.1",
    "vanilla-jsoneditor": "0.10.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "0.0.200",
    "@types/jest": "29.2.3",
    "@types/jsoneditor": "^9.9.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.5.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "fs-extra": "10.1.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "rollup": "2.79.1",
    "sass": "^1.56.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4",
    "ulid": "^2.3.0",
    "vite": "3.2.3",
    "ws": "8.11.0"
  }
}
