{
  "name": "searchable-network",
  "version": "1.0.0",
  "description": "A chrome extension primarily designed to help me and my team search in deep network payloads and responses",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghisloufou/searchable-network.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "buildw": "vite build -w",
    "typew": "tsc --noEmit -w --pretty",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (pnpm wss & nodemon)",
    "test": "jest",
    "start": "concurrently -n dev,typew,buildw \"pnpm devp\" \"pnpm typew\" \"pnpm buildw\""
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/jsonpath": "^0.2.0",
    "bootstrap": "^5.3.1",
    "jsoneditor": "^9.10.2",
    "jsonpath": "^1.1.1",
    "react": "18.2.0",
    "react-diff-viewer-continued": "^3.2.6",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-icons": "^4.10.1",
    "vanilla-jsoneditor": "0.10.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "0.0.243",
    "@types/jest": "29.5.4",
    "@types/jsoneditor": "^9.9.0",
    "@types/node": "20.5.3",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vitejs/plugin-react": "4.0.4",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "fs-extra": "11.1.1",
    "jest": "29.6.3",
    "jest-environment-jsdom": "29.6.3",
    "nodemon": "3.0.1",
    "prettier": "3.0.2",
    "rollup": "3.28.1",
    "sass": "^1.66.1",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "ulid": "^2.3.0",
    "vite": "4.4.9",
    "ws": "8.13.0"
  }
}
